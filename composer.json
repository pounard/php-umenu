{
    "name": "makinacorpus/php-umenu",
    "type": "drupal-module",
    "description": "Very simple CMS page menu tree handling API",
    "keywords": [],
    "homepage": "http://github.com/makinacorpus/php-umenu",
    "license": "GPL-2",
    "authors": [
        {
            "name": "Pierre Rineau",
            "email": "pierre.rineau@makina-corpus.com"
        },
        {
            "name": "Makina Corpus",
            "homepage": "http://makina-corpus.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:pounard/GeneratedHydrator.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pounard/goat.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pounard/goat-bundle.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require" : {
        "php": ">=7.1",
        "psr/cache" : "^1"
    },
    "require-dev": {
        "makinacorpus/goat": "dev-master",
        "makinacorpus/goat-bundle": "dev-master",
        "phpunit/phpunit" : "^6"
    },
    "suggests": {
        "makinacorpus/drupal-sf-dic ^3 || ^4": "Required for working in a Drupal environment",
        "makinacorpus/drupal-umenu ^3": "Required for working in a Drupal environment",
        "makinacorpus/goat": "Enables Goat storage backend",
        "makinacorpus/goat-bundle": "Uses Goat installer for managing database schema for the Goat storage backend"
    },
    "autoload" : {
        "psr-4" : {
            "MakinaCorpus\\Umenu\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "MakinaCorpus\\Umenu\\Tests\\" : "tests/"
        }
    }
}
